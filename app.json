{
  "expo": {
    "name": "learn",
    "slug": "learn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://cdn-icons-png.flaticon.com/128/9686/9686274.png",
    "scheme": "Online-Learning-Application",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://cdn-icons-png.flaticon.com/128/3749/3749770.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "https://cdn-icons-png.flaticon.com/128/3749/3749770.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "https://cdn-icons-png.flaticon.com/128/3749/3749770.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ]
  }
}
